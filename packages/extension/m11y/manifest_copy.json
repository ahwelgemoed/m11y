{
  "manifest_version": 3,
  "name": "M11Y-New",
  "description": "M11y-New",
  "version": "1.0",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "js/background.js"
  }
}
